.squid-table {
  @apply mx-auto my-8;
  @apply table-auto;
  @apply w-4/5;

  & .squid-table__header {
    @apply p-2;
    @apply font-bold;
    @apply text-center;
    @apply bg-blue-50;
    @apply border-b-2;
  }

  & .squid-table__cell {
    @apply p-2;
    @apply text-center;
  }

  & .squid-table__cell--blue {
    @apply bg-blue-50;
  }
}
